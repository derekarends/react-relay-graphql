{
  "name": "kinnl",
  "version": "1.0.0",
  "description": "Keep track of animals in kennels",
  "main": "index.js",
  "scripts": {
    "build": "npm run lint && npm run update-schema && npm run webpack",
    "lint": "eslint *.js src tools",
    "start": "gulp start-web-server",
    "update-schema": "babel-node tools/updateSchema.js",
    "webpack": "NODE_ENV=production webpack -p",
    "test": "./node_modules/.bin/jest",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test-debug": "node --debug-brk ./node_modules/.bin/jest -i --no-cache",
    "debug": "node --inspect --debug-brk ./dist/server/server-debug.js",
    "refresh": "gulp && npm run update-schema && npm run flow && npm run test && gulp && npm start"
  },
  "author": "Thinkovator Inc",
  "license": "ISC",
  "dependencies": {
    "babel-relay-plugin": "^0.10.0",
    "classnames": "^2.2.5",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "express-graphql": "^0.6.1",
    "graphql": "^0.8.2",
    "graphql-relay": "^0.4.4",
    "history": "^2.1.2",
    "mongodb": "^2.2.16",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-relay": "^0.10.0",
    "react-router": "^3.0.0",
    "react-router-relay": "^0.13.5"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "exports-loader": "^0.6.3",
    "flow-bin": "^0.37.4",
    "grunt-webpack": "^1.0.18",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-sass": "^3.0.0",
    "imports-loader": "^0.7.0",
    "jest-cli": "^18.0.0",
    "json-loader": "^0.5.4",
    "react-addons-test-utils": "^15.4.1",
    "sinon": "^1.17.6",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-stream": "^3.2.0"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  }
}
